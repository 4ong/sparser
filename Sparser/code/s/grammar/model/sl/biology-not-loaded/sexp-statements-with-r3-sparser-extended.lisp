       (:PMID . "22805337") (:SOURCE--API . "reach")
       (:ANNOTATIONS (:FOUND--BY . "Phosphorylation_syntax_3_noun"))
       (:EPISTEMICS (:SECTION--TYPE) (:DIRECT . T)))
      ((:TEXT
        . "Additionally, phosphorylation of P38MAPK and AKT, but not ERK1/2 or JNKs was specifically decreased in SGC7901 and DDP cells after ANXA2 siRNA delivery.")
       (:PMID . "25150310") (:SOURCE--API . "reach")
       (:ANNOTATIONS (:FOUND--BY . "Phosphorylation_syntax_3_noun"))
       (:EPISTEMICS (:SECTION--TYPE) (:DIRECT . T)))
      ((:TEXT
